{
	"name": "PimpMyPosts",
	"description": "Turn this decrepit thing into something cool",
	"version": "1.1",
	"manifest_version": 2,
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"64": "img/icon64.png",
		"128": "img/icon128.png"
	},
	"content_scripts": [{
		"matches": ["http://intra.42.fr/forum/*", "https://intra.42.fr/forum/*"],
		"js": ["js/lib/jquery-2.1.0.min.js", "js/lib/prism.js", "js/content.js"],
		"css": ["css/prism.css"]
	}],
	"web_accessible_resources": [
		"js/emotes.json",
		"img/emotes/*"
	]
}
